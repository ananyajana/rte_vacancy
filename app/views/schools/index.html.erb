<h1>Listing schools</h1>

<table>
  <tr>
    <th>User</th>
    <th>Reg</th>
    <th>School name</th>
    <th>Address</th>
    <th>District</th>
    <th>State</th>
    <th>Pin</th>
    <th>Phone</th>
    <th>Phone2</th>
    <th>Email</th>
    <th>Fax</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schools.each do |school| %>
  <tr>
    <td><%= school.user_id %></td>
    <td><%= school.reg_id %></td>
    <td><%= school.school_name %></td>
    <td><%= school.address %></td>
    <td><%= school.district %></td>
    <td><%= school.state %></td>
    <td><%= school.pin %></td>
    <td><%= school.phone %></td>
    <td><%= school.phone2 %></td>
    <td><%= school.email_id %></td>
    <td><%= school.fax %></td>
    <td><%= school.latitude %></td>
    <td><%= school.longitude %></td>
    <td><%= link_to 'Show', school %></td>
    <td><%= link_to 'Edit', edit_school_path(school) %></td>
    <td><%= link_to 'Destroy', school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New School', new_school_path %>
